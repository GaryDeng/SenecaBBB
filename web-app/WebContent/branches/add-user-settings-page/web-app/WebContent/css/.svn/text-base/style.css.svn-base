div.dataTables_wrapper {
	width: 900px;
	margin-top: 50px;
}
div.dataTables_wrapper:after {
	content: ".";
	height: 0;
	line-height: 0;
	clear: both;
	display: block;
	visibility: hidden;
}
table.pretty {
	width: 900px;
	clear: both;
	padding-top: 20px;
	padding-bottom: 10px;
}
/* Header cells */
table.pretty thead th {
	font-size: 9pt;
	font-family: "ITCAvantGardeStd Demi";
	text-align: left;
	color: #454545;
