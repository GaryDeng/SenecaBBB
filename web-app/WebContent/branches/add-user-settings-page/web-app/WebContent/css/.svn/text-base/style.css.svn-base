/* Pagination */
a.paginate_button, a.paginate_active {
	display: inline-block;
	padding: 10px;
	margin-right: 2px;
	width: 65px;
	font-size: 14pt;
	color: #FFFFFF;
	text-align: center;
	border: none;
	cursor: pointer;
	background-color: #CD352B;
	-webkit-transition: all 0.1s;
