}
a.paginate_button:hover {
	background-color: #9F100B;
}
a.paginate_button.paginate_button_disabled, a.paginate_button.paginate_button_disabled:hover {
	background-color: #bcbcbc;/*#E6E6E6;*/
}
a.paginate_active {
	background-color: #454545;
}
a.paginate_button_disabled {
}
.paging_full_numbers a:active {
	outline: none;
}
.paging_full_numbers a:hover {
	text-decoration: none;
}
div.dataTables_paginate span>a {
	width: 15px;
	text-align: center;
}
#page section article #example thead tr #darkCol {
	border-left: 1px solid #E0E0E0;
	padding-left: 14px;
}
#page section article #example thead tr #darkCol.sorting_asc, #page section article #example thead tr #darkCol.sorting_desc {
	border-left: 1px solid #454545;
}
#page section article #example thead tr #darkCol.sorting {
	border-left: 1px solid #E0E0E0;
}
#page section article #example tbody tr td a {
	margin-left: -9px;
}
#page section article #example tbody tr td a {
