	-ms-filter: hue-rotate(0deg);
	-o-filter: hue-rotate(0deg);*/
}
#page section article #example tbody tr td a#options:first-child {
	padding-right: 25px;
}
.icons {
	padding: 0px;
	margin: 0px;
	text-align: center;
}
.icons:after {
	display: none;
}

/* SELECT */
.select {
	height: 30px;
	width: 394px !important;
	float: right;
	display: block;
	padding: 0px;
	padding-top: 10px;
	padding-left: 10px;
	font-size: 13pt;
	font-family: "ITCAvantGardeStd BkCn";
	color: #838383;
	outline: none;
	border: none;
	position: relative; /* Enable absolute positionning for children and pseudo elements */
	cursor: pointer;
	background-color: #FFFFFF;
}
.select:after {
	content: "";
	width: 0;
	height: 0;
	position: absolute;
	right: 16px;
	top: 50%;
	margin-top: -6px;
	border-width: 6px 0 6px 6px;
	border-style: solid;
	border-color: transparent #E6322E;
}
.select .dropdown {
	position: absolute;
	top: 100%;
	left: 0;
	right: 0;
	list-style: none;
	opacity: 0;
	pointer-events: none;
	background-color: #F9F9F9;
}
.select .dropdown li a {
	display: block;
	text-decoration: none;
	color: #454545;
	padding: 10px 20px;
}
/* Hover state */
.select .dropdown li:hover a {
	background: #E4E4E4;
}
/* Active state */
.select.active .dropdown {
	opacity: 1;
	pointer-events: auto;
}
.select.active:after {
	border-color: #E6322E transparent;
	border-width: 6px 6px 0 6px;
	margin-top: -3px;
}
.select.active {
	background: #CD352B;/* Used colors: #9F100B; #CD352B; */
	color: #FFF;
	background: -moz-linear-gradient(left, #CD352B 0%, #CD352B 88%, #ffffff 88%, #ffffff 100%);
	background: -webkit-gradient(linear, left top, right top, color-stop(0%, #CD352B), color-stop(88%, #CD352B), color-stop(88%, #ffffff), color-stop(100%, #ffffff));
	background: -webkit-linear-gradient(left, #CD352B 0%, #CD352B 88%, #ffffff 88%, #ffffff 100%);
